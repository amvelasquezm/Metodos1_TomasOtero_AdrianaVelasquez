    "def RunSimulation(t):\n",
    "    \n",
    "    r0 = np.array([0.1,0.1])\n",
    "    v0 = np.array([40.,0])\n",
    "    a0 = np.array([0.,0])\n",
    "    \n",
    "    Limits = np.array([[-10.,10.],[-10.,10.]])\n",
    "    \n",
    "    p1 = Particle(r0,v0,a0,t)\n",
    "    \n",
    "    for it in tqdm(range(len(t))):\n",
    "        p1.Evolution(it)\n",
    "        p1.CheckLimits(Limits)\n",
    "        \n",
    "    return p1"
